@model IEnumerable<SettingNavbar>
<div class="container">
    <table class="table table-sm">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Icon:</th>
                <th scope="col">Icon:</th>
                <th scope="col">Icon:</th>
                <th scope="col">Başlıq:</th>
                <th scope="col">Başlıq:</th>
                <th scope="col">Başlıq:</th>
                <th scope="col">Başlıq:</th>
                <th scope="col">Başlıq:</th>
            
                <th scope="col">Yenile</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <th>@Html.Raw(item.Icon1)</th>
                        <th>@Html.Raw(item.Icon2)</th>
                        <th>@Html.Raw(item.Icon3)</th>
                        <th scope="row">@item.SiderbarP1</th>
                        <th scope="row">@item.SiderbarP2</th>
                        <th scope="row">@item.SiderbarP3</th>
                        <th scope="row">@item.SiderbarP4</th>
                        <th scope="row">@item.SiderbarP5</th>

                 


                        <td><a asp-controller="settingNavbar" asp-action="update" asp-route-id="@item.Id" >
                            <i class="fas fa-edit text text-primary"></i></a></td>
                     
                    </tr>
            }
        </tbody>
    </table>
  

</div>