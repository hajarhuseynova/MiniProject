@model IEnumerable<SettingNavbar>
<div class="container">
    <table class="table table-sm">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Icon:</th>
                <th scope="col">Icon:</th>
                <th scope="col">Icon:</th>
                <th scope="col">Sekil:</th>
                <th scope="col">Sekil:</th>
                <th scope="col">Sekil:</th>
                <th scope="col">Yenile</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <th>@Html.Raw(item.Icon1)</th>
                        <th>@Html.Raw(item.Icon2)</th>
                        <th>@Html.Raw(item.Icon3)</th>
                         <th><img style="width:100px;height:100px" src="~/assets/images/@item.Logo" /></th>
                         <th><img style="width:100px;height:100px" src="~/assets/images/@item.Hamburger" /></th>
                         <th><img style="width:100px;height:100px" src="~/assets/images/@item.Delete" /></th>
                     
                        <td><a asp-controller="settingNavbar" asp-action="update" asp-route-id="@item.Id" >
                            <i class="fas fa-edit text text-primary"></i></a></td>
                     
                    </tr>
            }
        </tbody>
    </table>
  

</div>