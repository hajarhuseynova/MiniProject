@model IEnumerable<SettingHomePage>
<div class="container">
    <table class="table table-sm">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Basliq:</th>
             
                <th scope="col">Sekil:</th>
                <th scope="col">Sekil:</th>
                <th scope="col">Sekil:</th>

                <th scope="col">Yenile</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <th>@item.TitleGift</th>
                         <th><img style="width:100px;height:100px" src="~/assets/images/@item.ImageGift1" /></th>
                         <th><img style="width:100px;height:100px" src="~/assets/images/@item.ImageGift2" /></th>
                         <th><img style="width:100px;height:100px" src="~/assets/images/@item.ImageTester" /></th>
                        <td><a asp-controller="settingHomePage" asp-action="update" asp-route-id="@item.Id" >
                            <i class="fas fa-edit text text-primary"></i></a></td>
                     
                    </tr>
            }
        </tbody>
    </table>
  

</div>