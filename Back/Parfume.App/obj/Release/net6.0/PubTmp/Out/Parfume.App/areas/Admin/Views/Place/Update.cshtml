@model Place

<div class="container">
	<form method="post" enctype="multipart/form-data">
	<div class="form-group">
		    <label asp-for="@Model.Title" >Adi:</label>
			<input type="text" class="form-control" asp-for="@Model.Title"/>
			<span class="text text-danger" asp-validation-for="@Model.Title"></span>
	</div>

	<div class="form-group">
		    <label asp-for="@Model.Address" >Addresi:</label>
			<input type="text" class="form-control" asp-for="@Model.Address"/>
			<span class="text text-danger" asp-validation-for="@Model.Address"></span>
	</div>

    <div class="form-group">
		    <label asp-for="@Model.Phone1" >Mobil No</label>
			<input type="text" class="form-control" asp-for="@Model.Phone1"/>
			<span class="text text-danger" asp-validation-for="@Model.Phone1"></span>
	</div>
	  <div class="form-group">
		    <label asp-for="@Model.Phone2" >Ev No</label>
			<input type="text" class="form-control" asp-for="@Model.Phone2"/>
			<span class="text text-danger" asp-validation-for="@Model.Phone2"></span>
	</div>

	 <div class="form-group">
		    <label asp-for="@Model.Location" >Yer Linki</label>
			<input type="text" class="form-control" asp-for="@Model.Location"/>
			<span class="text text-danger" asp-validation-for="@Model.Location"></span>
	</div>

	<div class="form-group">
		    <label asp-for="@Model.FormFile" >File:</label>
			<input type="file" class="form-control" asp-for="@Model.FormFile"/>
			<span class="text text-danger" asp-validation-for="@Model.FormFile"></span>
    </div>
			@Html.AntiForgeryToken()
	<button type="submit" class="btn btn-info">Yenile</button>
	</form>
</div>