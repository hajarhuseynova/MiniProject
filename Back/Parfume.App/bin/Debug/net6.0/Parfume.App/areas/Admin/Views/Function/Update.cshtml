@model Functions

<div class="container">
	<form method="post" enctype="multipart/form-data">
	<div class="form-group">
		    <label asp-for="@Model.Title" >Adi:</label>
			<input type="text" class="form-control" asp-for="@Model.Title"/>
			<span class="text text-danger" asp-validation-for="@Model.Title"></span>
	</div>

	<div class="form-group">
		    <label asp-for="@Model.Desc" >Movzu:</label>
			<input type="text" class="form-control" asp-for="@Model.Desc"/>
			<span class="text text-danger" asp-validation-for="@Model.Desc"></span>
	</div>

    <div class="form-group">
		    <label asp-for="@Model.Link" >Link</label>
			<input type="text" class="form-control" asp-for="@Model.Link"/>
			<span class="text text-danger" asp-validation-for="@Model.Link"></span>
	</div>
	
		<div class="form-group">
		    <label asp-for="@Model.FormFile" >File:</label>
			<input type="file" class="form-control" asp-for="@Model.FormFile"/>
			<span class="text text-danger" asp-validation-for="@Model.FormFile"></span>
    </div>
	
			@Html.AntiForgeryToken()
	<button type="submit" class="btn btn-info">Yenile</button>
	</form>
</div>